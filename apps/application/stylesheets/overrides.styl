.progress-bar-dull
    background-color: #535353

.progress-bar-blue-sky
    background-color: #618CBD

.btn
    border-radius: 0
    color: #000

pre
    background-color: transparent
    border-color: transparent
    color: initial
    line-height: 1.42857143
    margin: 0
    padding: 0
    white-space: pre
    code
        white-space: inherit
        word-wrap: normal

code
    font-size: 80%
    color: rgb(61, 61, 61)
    background-color: rgba(0,0,0,0.04)
    border-radius: 4px

.hljs.bash
    *
        color: #839496
